{
  "name": "webPage",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "pageName": {
      "type": "string",
      "required": true
    },
    "pageUrl": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "website": {
      "type": "belongsTo",
      "model": "website",
      "foreignKey": "website_id"
    },
    "editableFields": {
      "type": "hasMany",
      "model": "editableField",
      "foreignKey": "webPage_id"
    },
    "systemAdmin": {
      "type": "belongsTo",
      "model": "systemAdmin",
      "foreignKey": "systemAdmin_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
